{
    "family": "medusa-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::123456789012:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "medusa",
        "image": "dummy",
        "portMappings": [
          {
            "containerPort": 9000,
            "hostPort": 9000,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
  }
  